---
import MainLayout from '../../layouts/MainLayout.astro';
import Breadcrumb from '../../components/Breadcrumb.astro';

const breadcrumbItems = [
  { name: 'Home', href: '/' },
  { name: 'Molex Compatible', href: '/compatibility/molex-compatible' }
];
---

<MainLayout
  title="Molex Compatible Cable Assembly | Cost-Effective Alternative Solutions"
  description="Molex compatible cable assemblies at factory-direct prices. Micro-Fit, PicoBlade, Mini-Fit, KK alternatives. Same quality, 30-50% cost savings. Quick-turn delivery."
>
  <script type="application/ld+json" set:html={JSON.stringify({
    "@context": "https://schema.org",
    "@type": "Product",
    "name": "Molex Compatible Cable Assembly",
    "description": "Cost-effective Molex-compatible wire harness assemblies for various Molex connector series",
    "brand": {
      "@type": "Brand",
      "name": "Trycay"
    },
    "offers": {
      "@type": "Offer",
      "availability": "https://schema.org/InStock",
      "priceCurrency": "USD"
    }
  })} />

  <Breadcrumb items={breadcrumbItems} />

  <section class="px-4 py-16">
    <div class="max-w-5xl mx-auto">
      <div class="mb-12">
        <p class="font-mono text-xs text-accent-cyan uppercase tracking-widest mb-4">Connector Compatibility</p>
        <h1 class="text-4xl md:text-5xl font-bold tracking-tight mb-6">
          Molex Compatible <span class="text-accent-cyan">Cable Assembly</span>
        </h1>
        <p class="text-xl text-text-secondary leading-relaxed">
          Complete range of Molex-compatible cable assemblies offering the same quality and reliability as genuine Molex connectors at 30-50% lower cost. Direct from our ISO 9001 certified factory.
        </p>
      </div>

      <!-- Why Choose Compatible -->
      <section class="mb-16">
        <h2 class="text-2xl font-bold mb-6">Why Choose Molex Compatible Solutions?</h2>
        <div class="grid grid-cols-1 md:grid-cols-3 gap-6">
          <div class="bg-bg-secondary border border-border p-6">
            <div class="font-mono text-3xl font-bold text-accent-green mb-2">30-50%</div>
            <div class="font-bold mb-2">Cost Savings</div>
            <p class="text-text-secondary text-sm">Significant cost reduction without compromising quality or performance compared to branded alternatives.</p>
          </div>
          <div class="bg-bg-secondary border border-border p-6">
            <div class="font-mono text-3xl font-bold text-accent-green mb-2">100%</div>
            <div class="font-bold mb-2">Compatibility</div>
            <p class="text-text-secondary text-sm">Fully compatible with existing Molex housings, headers, and mating connectors.</p>
          </div>
          <div class="bg-bg-secondary border border-border p-6">
            <div class="font-mono text-3xl font-bold text-accent-green mb-2">48h</div>
            <div class="font-bold mb-2">Quick Delivery</div>
            <p class="text-text-secondary text-sm">Rapid prototyping and production from our dedicated manufacturing facility.</p>
          </div>
        </div>
      </section>

      <!-- Molex Series Overview -->
      <section class="mb-16">
        <h2 class="text-2xl font-bold mb-6">Available Molex Series Equivalents</h2>
        <div class="space-y-6">
          <!-- Micro-Fit -->
          <div class="bg-bg-secondary border border-border p-6">
            <div class="flex items-start gap-6">
              <div class="w-16 h-16 bg-accent-cyan/10 border border-accent-cyan/30 flex items-center justify-center flex-shrink-0">
                <span class="font-mono text-sm font-bold text-accent-cyan text-center leading-tight">MICRO-FIT</span>
              </div>
              <div class="flex-1">
                <h3 class="text-xl font-bold mb-2">Micro-Fit 3.0</h3>
                <p class="text-text-secondary text-sm mb-4">3.0mm pitch higher current capacity connectors. Ideal for power delivery and higher current applications.</p>
                <div class="grid grid-cols-2 md:grid-cols-4 gap-4 mb-4">
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Pitch</div>
                    <div class="font-bold">3.0mm</div>
                  </div>
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Circuits</div>
                    <div class="font-bold">2-12</div>
                  </div>
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Voltage</div>
                    <div class="font-bold">300V</div>
                  </div>
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Current</div>
                    <div class="font-bold">5A</div>
                  </div>
                </div>
                <div class="flex gap-2">
                  <span class="font-mono text-xs px-2 py-1 bg-accent-amber/10 border border-accent-amber/30 text-accent-amber">Higher Power</span>
                  <span class="font-mono text-xs px-2 py-1 bg-accent-amber/10 border border-accent-amber/30 text-accent-amber">Secure Lock</span>
                </div>
              </div>
            </div>
          </div>

          <!-- PicoBlade -->
          <div class="bg-bg-secondary border border-border p-6">
            <div class="flex items-start gap-6">
              <div class="w-16 h-16 bg-accent-cyan/10 border border-accent-cyan/30 flex items-center justify-center flex-shrink-0">
                <span class="font-mono text-sm font-bold text-accent-cyan text-center leading-tight">PICOBLADE</span>
              </div>
              <div class="flex-1">
                <h3 class="text-xl font-bold mb-2">PicoBlade</h3>
                <p class="text-text-secondary text-sm mb-4">1.25mm ultra-fine pitch connectors for compact and miniature applications.</p>
                <div class="grid grid-cols-2 md:grid-cols-4 gap-4 mb-4">
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Pitch</div>
                    <div class="font-bold">1.25mm</div>
                  </div>
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Circuits</div>
                    <div class="font-bold">2-12</div>
                  </div>
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Voltage</div>
                    <div class="font-bold">100V</div>
                  </div>
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Current</div>
                    <div class="font-bold">1A</div>
                  </div>
                </div>
                <div class="flex gap-2">
                  <span class="font-mono text-xs px-2 py-1 bg-accent-amber/10 border border-accent-amber/30 text-accent-amber">Ultra-Compact</span>
                  <span class="font-mono text-xs px-2 py-1 bg-accent-amber/10 border border-accent-amber/30 text-accent-amber">Low Profile</span>
                </div>
              </div>
            </div>
          </div>

          <!-- Mini-Fit -->
          <div class="bg-bg-secondary border border-border p-6">
            <div class="flex items-start gap-6">
              <div class="w-16 h-16 bg-accent-cyan/10 border border-accent-cyan/30 flex items-center justify-center flex-shrink-0">
                <span class="font-mono text-sm font-bold text-accent-cyan text-center leading-tight">MINI-FIT</span>
              </div>
              <div class="flex-1">
                <h3 class="text-xl font-bold mb-2">Mini-Fit JR & BMI</h3>
                <p class="text-text-secondary text-sm mb-4">4.20mm pitch high power connectors for demanding power applications.</p>
                <div class="grid grid-cols-2 md:grid-cols-4 gap-4 mb-4">
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Pitch</div>
                    <div class="font-bold">4.20mm</div>
                  </div>
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Circuits</div>
                    <div class="font-bold">2-16</div>
                  </div>
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Voltage</div>
                    <div class="font-bold">600V</div>
                  </div>
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Current</div>
                    <div class="font-bold">13A</div>
                  </div>
                </div>
                <div class="flex gap-2">
                  <span class="font-mono text-xs px-2 py-1 bg-accent-amber/10 border border-accent-amber/30 text-accent-amber">High Power</span>
                  <span class="font-mono text-xs px-2 py-1 bg-accent-amber/10 border border-accent-amber/30 text-accent-amber">Robust</span>
                </div>
              </div>
            </div>
          </div>

          <!-- KK Series -->
          <div class="bg-bg-secondary border border-border p-6">
            <div class="flex items-start gap-6">
              <div class="w-16 h-16 bg-accent-cyan/10 border border-accent-cyan/30 flex items-center justify-center flex-shrink-0">
                <span class="font-mono text-sm font-bold text-accent-cyan text-center leading-tight">KK</span>
              </div>
              <div class="flex-1">
                <h3 class="text-xl font-bold mb-2">KK Series</h3>
                <p class="text-text-secondary text-sm mb-4">3.96mm pitch general purpose connectors for wire-to-board and wire-to-wire connections.</p>
                <div class="grid grid-cols-2 md:grid-cols-4 gap-4 mb-4">
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Pitch</div>
                    <div class="font-bold">3.96mm</div>
                  </div>
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Circuits</div>
                    <div class="font-bold">2-15</div>
                  </div>
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Voltage</div>
                    <div class="font-bold">250V</div>
                  </div>
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Current</div>
                    <div class="font-bold">7A</div>
                  </div>
                </div>
                <div class="flex gap-2">
                  <span class="font-mono text-xs px-2 py-1 bg-accent-amber/10 border border-accent-amber/30 text-accent-amber">Versatile</span>
                  <span class="font-mono text-xs px-2 py-1 bg-accent-amber/10 border border-accent-amber/30 text-accent-amber">Widely Used</span>
                </div>
              </div>
            </div>
          </div>

          <!-- Sabre -->
          <div class="bg-bg-secondary border border-border p-6">
            <div class="flex items-start gap-6">
              <div class="w-16 h-16 bg-accent-cyan/10 border border-accent-cyan/30 flex items-center justify-center flex-shrink-0">
                <span class="font-mono text-sm font-bold text-accent-cyan text-center leading-tight">SABRE</span>
              </div>
              <div class="flex-1">
                <h3 class="text-xl font-bold mb-2">Sabre</h3>
                <p class="text-text-secondary text-sm mb-4">2.5mm pitch single row connectors with positive lock for general applications.</p>
                <div class="grid grid-cols-2 md:grid-cols-4 gap-4 mb-4">
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Pitch</div>
                    <div class="font-bold">2.5mm</div>
                  </div>
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Circuits</div>
                    <div class="font-bold">2-10</div>
                  </div>
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Voltage</div>
                    <div class="font-bold">250V</div>
                  </div>
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Current</div>
                    <div class="font-bold">3A</div>
                  </div>
                </div>
                <div class="flex gap-2">
                  <span class="font-mono text-xs px-2 py-1 bg-accent-amber/10 border border-accent-amber/30 text-accent-amber">Positive Lock</span>
                  <span class="font-mono text-xs px-2 py-1 bg-accent-amber/10 border border-accent-amber/30 text-accent-amber">Low Profile</span>
                </div>
              </div>
            </div>
          </div>
        </div>
      </section>

      <!-- Quality & Compatibility -->
      <section class="mb-16">
        <h2 class="text-2xl font-bold mb-6">Quality & Compatibility Assurance</h2>
        <div class="grid grid-cols-1 md:grid-cols-2 gap-6">
          <div class="border-l-2 border-accent-cyan pl-6">
            <h3 class="font-bold mb-2">Dimensional Precision</h3>
            <p class="text-text-secondary text-sm">Our compatible connectors are manufactured to exact dimensional specifications ensuring perfect mating with Molex housings and headers.</p>
          </div>
          <div class="border-l-2 border-accent-cyan pl-6">
            <h3 class="font-bold mb-2">Material Quality</h3>
            <p class="text-text-secondary text-sm">High-quality materials with UL94V-0 flame rating and RoHS compliance for safety and environmental compliance.</p>
          </div>
          <div class="border-l-2 border-accent-cyan pl-6">
            <h3 class="font-bold mb-2">Electrical Performance</h3>
            <p class="text-text-secondary text-sm">Current and voltage ratings meet or exceed Molex specifications for reliable power and signal delivery.</p>
          </div>
          <div class="border-l-2 border-accent-cyan pl-6">
            <h3 class="font-bold mb-2">100% Testing</h3>
            <p class="text-text-secondary text-sm">Every assembly undergoes continuity testing, pull testing, and visual inspection for consistent quality.</p>
          </div>
        </div>
      </section>

      <!-- Applications -->
      <section class="mb-16">
        <h2 class="text-2xl font-bold mb-6">Applications Using Molex Compatible Connectors</h2>
        <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-6">
          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold mb-2">Power Supply Units</h3>
            <p class="text-text-secondary text-sm">PSU internal wiring, power distribution, and cable assemblies for various power applications.</p>
          </div>
          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold mb-2">Industrial Controls</h3>
            <p class="text-text-secondary text-sm">PLC systems, motor controls, and automation equipment with high power requirements.</p>
          </div>
          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold mb-2">LED Lighting</h3>
            <p class="text-text-secondary text-sm">LED driver connections and power delivery for high-power LED arrays and fixtures.</p>
          </div>
          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold mb-2">Automotive Electronics</h3>
            <p class="text-text-secondary text-sm">Automotive infotainment, dashboard modules, and vehicle control systems.</p>
          </div>
          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold mb-2">Telecommunications</h3>
            <p class="text-text-secondary text-sm">Network equipment, power delivery, and internal wiring for telecom hardware.</p>
          </div>
          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold mb-2">Computer Hardware</h3>
            <p class="text-text-secondary text-sm">Motherboard power connectors, peripheral internal wiring, and computer power supplies.</p>
          </div>
        </div>
      </section>

      <!-- CTA -->
      <section class="bg-bg-secondary border border-accent-cyan p-8 text-center">
        <h2 class="text-2xl font-bold mb-4">Ready to Save on Molex Compatible Cable Assemblies?</h2>
        <p class="text-text-secondary mb-6 max-w-2xl mx-auto">
          Get a competitive quote within 24 hours. Share your Molex part numbers or specifications for cross-reference and pricing.
        </p>
        <a href="/products/molex-compatible-wire-assembly" class="inline-block px-8 py-4 font-mono text-sm font-bold uppercase tracking-widest text-bg-primary bg-accent-cyan hover:bg-accent-green transition-all duration-300">
          View Molex Compatible Products
        </a>
      </section>
    </div>
  </section>
</MainLayout>
