---
import MainLayout from '../../layouts/MainLayout.astro';
import Breadcrumb from '../../components/Breadcrumb.astro';

const breadcrumbItems = [
  { name: 'Home', href: '/' },
  { name: 'JST Compatible', href: '/compatibility/jst-compatible' },
  { name: 'JST PH Cable Assembly', href: '/compatibility/jst-ph-cable-assembly' }
];
---

<MainLayout
  title="JST PH Series Cable Assembly | 2.0mm Pitch Wire Harness Manufacturer"
  description="JST PH compatible cable assemblies. 2.0mm pitch wire-to-board connectors. Factory-direct pricing, custom configurations, 48-hour sampling. Full range of PH series housings, headers, terminals."
>
  <script type="application/ld+json" set:html={JSON.stringify({
    "@context": "https://schema.org",
    "@type": "Product",
    "name": "JST PH Series Cable Assembly",
    "description": "2.0mm pitch wire-to-board cable assemblies compatible with JST PH series connectors",
    "brand": {
      "@type": "Brand",
      "name": "Trycay"
    },
    "offers": {
      "@type": "Offer",
      "availability": "https://schema.org/InStock",
      "priceCurrency": "USD"
    }
  })} />

  <Breadcrumb items={breadcrumbItems} />

  <section class="px-4 py-16">
    <div class="max-w-5xl mx-auto">
      <div class="mb-12">
        <p class="font-mono text-xs text-accent-cyan uppercase tracking-widest mb-4">PH Series Compatibility</p>
        <h1 class="text-4xl md:text-5xl font-bold tracking-tight mb-6">
          JST PH Series <span class="text-accent-cyan">Cable Assembly</span>
        </h1>
        <p class="text-xl text-text-secondary leading-relaxed">
          Complete range of JST PH-compatible cable assemblies with 2.0mm pitch wire-to-board connectors. Custom wire lengths, configurations, and quick-turn delivery from our ISO 9001 certified factory.
        </p>
      </div>

      <!-- Quick Specs -->
      <section class="mb-16">
        <div class="bg-bg-secondary border border-accent-cyan p-8">
          <h2 class="text-2xl font-bold mb-6">JST PH Series Specifications</h2>
          <div class="grid grid-cols-2 md:grid-cols-4 gap-6">
            <div>
              <div class="font-mono text-xs text-text-muted uppercase tracking-wider mb-1">Pitch</div>
              <div class="font-mono text-2xl font-bold text-accent-cyan">2.0mm</div>
            </div>
            <div>
              <div class="font-mono text-xs text-text-muted uppercase tracking-wider mb-1">Positions</div>
              <div class="font-mono text-2xl font-bold text-accent-cyan">2-15</div>
            </div>
            <div>
              <div class="font-mono text-xs text-text-muted uppercase tracking-wider mb-1">Voltage Rating</div>
              <div class="font-mono text-2xl font-bold text-accent-cyan">100V</div>
            </div>
            <div>
              <div class="font-mono text-xs text-text-muted uppercase tracking-wider mb-1">Current Rating</div>
              <div class="font-mono text-2xl font-bold text-accent-cyan">2A</div>
            </div>
          </div>
        </div>
      </section>

      <!-- Product Types -->
      <section class="mb-16">
        <h2 class="text-2xl font-bold mb-6">PH Series Product Range</h2>
        <div class="grid grid-cols-1 md:grid-cols-2 gap-6">
          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold text-lg mb-3">PH Series Receptacle Housings</h3>
            <p class="text-text-secondary text-sm mb-4">Wire-side housings with friction lock mechanism for secure connections.</p>
            <div class="space-y-3">
              <div class="flex justify-between items-center text-sm">
                <span class="text-text-secondary">2-Pin Housing</span>
                <span class="font-mono text-xs text-accent-cyan">SFR-001</span>
              </div>
              <div class="flex justify-between items-center text-sm">
                <span class="text-text-secondary">3-Pin Housing</span>
                <span class="font-mono text-xs text-accent-cyan">SFR-002</span>
              </div>
              <div class="flex justify-between items-center text-sm">
                <span class="text-text-secondary">4-Pin Housing</span>
                <span class="font-mono text-xs text-accent-cyan">SFR-004</span>
              </div>
              <div class="flex justify-between items-center text-sm">
                <span class="text-text-secondary">5-Pin Housing</span>
                <span class="font-mono text-xs text-accent-cyan">SFR-005</span>
              </div>
              <div class="flex justify-between items-center text-sm">
                <span class="text-text-secondary">6-Pin to 15-Pin</span>
                <span class="font-mono text-xs text-accent-cyan">Available</span>
              </div>
            </div>
          </div>

          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold text-lg mb-3">PH Series PCB Headers</h3>
            <p class="text-text-secondary text-sm mb-4">Through-hole and surface mount headers for PCB mounting.</p>
            <div class="space-y-3">
              <div class="flex justify-between items-center text-sm">
                <span class="text-text-secondary">2-Pin Header THT</span>
                <span class="font-mono text-xs text-accent-cyan">HDR-PH02</span>
              </div>
              <div class="flex justify-between items-center text-sm">
                <span class="text-text-secondary">3-Pin Header THT</span>
                <span class="font-mono text-xs text-accent-cyan">HDR-PH03</span>
              </div>
              <div class="flex justify-between items-center text-sm">
                <span class="text-text-secondary">4-Pin Header THT</span>
                <span class="font-mono text-xs text-accent-cyan">HDR-PH04</span>
              </div>
              <div class="flex justify-between items-center text-sm">
                <span class="text-text-secondary">SMT Options</span>
                <span class="font-mono text-xs text-accent-cyan">Available</span>
              </div>
              <div class="flex justify-between items-center text-sm">
                <span class="text-text-secondary">Right-Angle</span>
                <span class="font-mono text-xs text-accent-cyan">Available</span>
              </div>
            </div>
          </div>

          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold text-lg mb-3">PH Series Crimp Terminals</h3>
            <p class="text-text-secondary text-sm mb-4">Tin-plated copper terminals for reliable wire crimp connections.</p>
            <ul class="space-y-2 text-sm text-text-secondary">
              <li class="flex items-start gap-2"><span class="text-accent-cyan">▸</span> Wire gauge: 28-24 AWG standard</li>
              <li class="flex items-start gap-2"><span class="text-accent-cyan">▸</span> Material: Tin-plated copper</li>
              <li class="flex items-start gap-2"><span class="text-accent-cyan">▸</span> Packaging: Bulk or reel</li>
              <li class="flex items-start gap-2"><span class="text-accent-cyan">▸</span> Crimp tooling available</li>
            </ul>
          </div>

          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold text-lg mb-3">Custom PH Cable Assemblies</h3>
            <p class="text-text-secondary text-sm mb-4">Complete cable assemblies with PH connectors on one or both ends.</p>
            <ul class="space-y-2 text-sm text-text-secondary">
              <li class="flex items-start gap-2"><span class="text-accent-cyan">▸</span> Custom wire lengths</li>
              <li class="flex items-start gap-2"><span class="text-accent-cyan">▸</span> Various wire gauges</li>
              <li class="flex items-start gap-2"><span class="text-accent-cyan">▸</span> Color-coded wires</li>
              <li class="flex items-start gap-2"><span class="text-accent-cyan">▸</span> Strain relief options</li>
              <li class="flex items-start gap-2"><span class="text-accent-cyan">▸</span> Mixed position counts</li>
            </ul>
          </div>
        </div>
      </section>

      <!-- Key Features -->
      <section class="mb-16">
        <h2 class="text-2xl font-bold mb-6">Key Features of PH Series</h2>
        <div class="grid grid-cols-1 md:grid-cols-2 gap-6">
          <div class="flex gap-4">
            <div class="w-12 h-12 flex-shrink-0 bg-accent-cyan/10 border border-accent-cyan/30 flex items-center justify-center text-accent-cyan">
              <svg class="w-6 h-6" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" d="M19 11H5m14 0a2 2 0 012 2v6a2 2 0 01-2 2H5a2 2 0 01-2-2v-6a2 2 0 012-2m14 0V9a2 2 0 00-2-2M5 11V9a2 2 0 012-2m0 0V5a2 2 0 012-2h6a2 2 0 012 2v2M7 7h10" />
              </svg>
            </div>
            <div>
              <h3 class="font-bold mb-2">Ultra-Compact 2.0mm Pitch</h3>
              <p class="text-text-secondary text-sm">Industry-leading compact design for high-density PCB layouts in space-constrained devices.</p>
            </div>
          </div>
          <div class="flex gap-4">
            <div class="w-12 h-12 flex-shrink-0 bg-accent-cyan/10 border border-accent-cyan/30 flex items-center justify-center text-accent-cyan">
              <svg class="w-6 h-6" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" d="M12 15v2m-6 4h12a2 2 0 002-2v-6a2 2 0 00-2-2H6a2 2 0 00-2 2v6a2 2 0 002 2zm10-10V7a4 4 0 00-8 0v4h8z" />
              </svg>
            </div>
            <div>
              <h3 class="font-bold mb-2">Friction Lock Design</h3>
              <p class="text-text-secondary text-sm">Secure connection retention without additional locking mechanisms.</p>
            </div>
          </div>
          <div class="flex gap-4">
            <div class="w-12 h-12 flex-shrink-0 bg-accent-cyan/10 border border-accent-cyan/30 flex items-center justify-center text-accent-cyan">
              <svg class="w-6 h-6" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z" />
              </svg>
            </div>
            <div>
              <h3 class="font-bold mb-2">PA66 GF Material</h3>
              <p class="text-text-secondary text-sm">Glass-filled polyamide housing with UL94V-0 flame rating for safety.</p>
            </div>
          </div>
          <div class="flex gap-4">
            <div class="w-12 h-12 flex-shrink-0 bg-accent-cyan/10 border border-accent-cyan/30 flex items-center justify-center text-accent-cyan">
              <svg class="w-6 h-6" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" d="M4 5a1 1 0 011-1h14a1 1 0 011 1v2a1 1 0 01-1 1H5a1 1 0 01-1-1V5zM4 13a1 1 0 011-1h6a1 1 0 011 1v6a1 1 0 01-1 1H5a1 1 0 01-1-1v-6zM16 13a1 1 0 011-1h2a1 1 0 011 1v6a1 1 0 01-1 1h-2a1 1 0 01-1-1v-6z" />
              </svg>
            </div>
            <div>
              <h3 class="font-bold mb-2">Multi-Position Options</h3>
              <p class="text-text-secondary text-sm">Available in 2 through 15 position configurations for various application requirements.</p>
            </div>
          </div>
        </div>
      </section>

      <!-- Applications -->
      <section class="mb-16">
        <h2 class="text-2xl font-bold mb-6">PH Series Applications</h2>
        <div class="grid grid-cols-1 md:grid-cols-3 gap-6">
          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold mb-2">Consumer Electronics</h3>
            <p class="text-text-secondary text-sm">Smartphones, tablets, wearables, portable devices requiring compact connections.</p>
          </div>
          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold mb-2">Computer Peripherals</h3>
            <p class="text-text-secondary text-sm">Internal PCB connections, power delivery, signal routing in computer equipment.</p>
          </div>
          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold mb-2">LED Lighting</h3>
            <p class="text-text-secondary text-sm">LED strip connections, low-profile lighting fixtures, compact driver interfaces.</p>
          </div>
          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold mb-2">Automotive Electronics</h3>
            <p class="text-text-secondary text-sm">Dashboard modules, sensor connections, infotainment systems.</p>
          </div>
          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold mb-2">IoT Devices</h3>
            <p class="text-text-secondary text-sm">Smart home devices, sensors, controllers with space constraints.</p>
          </div>
          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold mb-2">Medical Devices</h3>
            <p class="text-text-secondary text-sm">Portable medical equipment, patient monitoring systems.</p>
          </div>
        </div>
      </section>

      <!-- Customization -->
      <section class="mb-16">
        <h2 class="text-2xl font-bold mb-6">Custom PH Cable Assembly Options</h2>
        <div class="bg-bg-secondary border border-border p-6">
          <table class="w-full text-sm">
            <thead>
              <tr class="border-b border-border">
                <th class="text-left py-3 font-mono text-xs text-accent-cyan uppercase">Option</th>
                <th class="text-left py-3 font-mono text-xs text-accent-cyan uppercase">Choices</th>
              </tr>
            </thead>
            <tbody class="text-text-secondary">
              <tr class="border-b border-border/50">
                <td class="py-3 font-medium">Wire Length</td>
                <td class="py-3">Custom lengths from 10mm to 5 meters</td>
              </tr>
              <tr class="border-b border-border/50">
                <td class="py-3 font-medium">Wire Gauge</td>
                <td class="py-3">32 AWG to 20 AWG (application dependent)</td>
              </tr>
              <tr class="border-b border-border/50">
                <td class="py-3 font-medium">Wire Color</td>
                <td class="py-3">Standard colors or custom color coding</td>
              </tr>
              <tr class="border-b border-border/50">
                <td class="py-3 font-medium">Strain Relief</td>
                <td class="py-3">Overmold, heat shrink, or mechanical</td>
              </tr>
              <tr class="border-b border-border/50">
                <td class="py-3 font-medium">Shielding</td>
                <td class="py-3">Foil, braid, or foil + braid options</td>
              </tr>
              <tr class="border-b border-border/50">
                <td class="py-3 font-medium">Connector Configurations</td>
                <td class="py-3">PH-PH, PH-wire open, mixed series</td>
              </tr>
              <tr>
                <td class="py-3 font-medium">Labeling</td>
                <td class="py-3">Custom labels and markings available</td>
              </tr>
            </tbody>
          </table>
        </div>
      </section>

      <!-- CTA -->
      <section class="bg-bg-secondary border border-accent-cyan p-8 text-center">
        <h2 class="text-2xl font-bold mb-4">Need JST PH Compatible Cable Assemblies?</h2>
        <p class="text-text-secondary mb-6 max-w-2xl mx-auto">
          Share your PH series requirements for a competitive quote within 24 hours. Specify position count, wire length, and quantity.
        </p>
        <a href="/contact" class="inline-block px-8 py-4 font-mono text-sm font-bold uppercase tracking-widest text-bg-primary bg-accent-cyan hover:bg-accent-green transition-all duration-300">
          Get PH Series Quote
        </a>
      </section>
    </div>
  </section>
</MainLayout>
