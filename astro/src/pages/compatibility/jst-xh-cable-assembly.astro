---
import MainLayout from '../../layouts/MainLayout.astro';
import Breadcrumb from '../../components/Breadcrumb.astro';

const breadcrumbItems = [
  { name: 'Home', href: '/' },
  { name: 'JST Compatible', href: '/compatibility/jst-compatible' },
  { name: 'JST XH Wire Harness', href: '/compatibility/jst-xh-cable-assembly' }
];
---

<MainLayout
  title="JST XH Series Wire Harness | 2.5mm Pitch Cable Assembly Manufacturer"
  description="JST XH compatible wire harness and cable assemblies. 2.5mm pitch with positive lock. Industrial control, LED, power supply applications. Factory-direct, custom configurations."
>
  <Breadcrumb items={breadcrumbItems} />

  <section class="px-4 py-16">
    <div class="max-w-5xl mx-auto">
      <div class="mb-12">
        <p class="font-mono text-xs text-accent-cyan uppercase tracking-widest mb-4">XH Series Compatibility</p>
        <h1 class="text-4xl md:text-5xl font-bold tracking-tight mb-6">
          JST XH Series <span class="text-accent-cyan">Wire Harness</span>
        </h1>
        <p class="text-xl text-text-secondary leading-relaxed">
          JST XH-compatible wire harnesses with 2.5mm pitch and positive locking mechanism. Widely used in industrial control, LED lighting, and power supply applications. Custom manufacturing from our ISO 9001 certified facility.
        </p>
      </div>

      <!-- Quick Specs -->
      <section class="mb-16">
        <div class="bg-bg-secondary border border-accent-cyan p-8">
          <h2 class="text-2xl font-bold mb-6">JST XH Series Specifications</h2>
          <div class="grid grid-cols-2 md:grid-cols-4 gap-6">
            <div>
              <div class="font-mono text-xs text-text-muted uppercase tracking-wider mb-1">Pitch</div>
              <div class="font-mono text-2xl font-bold text-accent-cyan">2.5mm</div>
            </div>
            <div>
              <div class="font-mono text-xs text-text-muted uppercase tracking-wider mb-1">Positions</div>
              <div class="font-mono text-2xl font-bold text-accent-cyan">2-15</div>
            </div>
            <div>
              <div class="font-mono text-xs text-text-muted uppercase tracking-wider mb-1">Voltage Rating</div>
              <div class="font-mono text-2xl font-bold text-accent-cyan">250V</div>
            </div>
            <div>
              <div class="font-mono text-xs text-text-muted uppercase tracking-wider mb-1">Current Rating</div>
              <div class="font-mono text-2xl font-bold text-accent-cyan">3A</div>
            </div>
          </div>
        </div>
      </section>

      <!-- Why XH Series -->
      <section class="mb-16">
        <h2 class="text-2xl font-bold mb-6">Why Choose JST XH Series?</h2>
        <div class="grid grid-cols-1 md:grid-cols-2 gap-6">
          <div class="bg-bg-secondary border border-border p-6">
            <div class="flex items-start gap-4">
              <div class="w-12 h-12 bg-accent-cyan/10 flex items-center justify-center flex-shrink-0">
                <svg class="w-6 h-6 text-accent-cyan" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" d="M12 15v2m-6 4h12a2 2 0 002-2v-6a2 2 0 00-2-2H6a2 2 0 00-2 2v6a2 2 0 002 2zm10-10V7a4 4 0 00-8 0v4h8z" />
                </svg>
              </div>
              <div>
                <h3 class="font-bold mb-2">Positive Lock Mechanism</h3>
                <p class="text-text-secondary text-sm">Secure locking prevents accidental disconnection in vibration-prone industrial environments.</p>
              </div>
            </div>
          </div>
          <div class="bg-bg-secondary border border-border p-6">
            <div class="flex items-start gap-4">
              <div class="w-12 h-12 bg-accent-cyan/10 flex items-center justify-center flex-shrink-0">
                <svg class="w-6 h-6 text-accent-cyan" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" d="M13 10V3L4 14h7v7l9-11h-7z" />
                </svg>
              </div>
              <div>
                <h3 class="font-bold mb-2">Higher Power Capacity</h3>
                <p class="text-text-secondary text-sm">3A current rating suitable for power delivery and LED driver applications.</p>
              </div>
            </div>
          </div>
          <div class="bg-bg-secondary border border-border p-6">
            <div class="flex items-start gap-4">
              <div class="w-12 h-12 bg-accent-cyan/10 flex items-center justify-center flex-shrink-0">
                <svg class="w-6 h-6 text-accent-cyan" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" d="M9 12l2 2 4-4m5.618-4.016A11.955 11.955 0 0112 2.944a11.955 11.955 0 01-8.618 3.04A12.02 12.02 0 003 9c0 5.591 3.824 10.29 9 11.622 5.176-1.332 9-6.03 9-11.622 0-1.042-.133-2.052-.382-3.016z" />
                </svg>
              </div>
              <div>
                <h3 class="font-bold mb-2">Industrial Reliability</h3>
                <p class="text-text-secondary text-sm">Proven performance in industrial automation and control applications worldwide.</p>
              </div>
            </div>
          </div>
          <div class="bg-bg-secondary border border-border p-6">
            <div class="flex items-start gap-4">
              <div class="w-12 h-12 bg-accent-cyan/10 flex items-center justify-center flex-shrink-0">
                <svg class="w-6 h-6 text-accent-cyan" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" d="M12 8c-1.657 0-3 .895-3 2s1.343 2 3 2 3 .895 3 2-1.343 2-3 2m0-8c1.11 0 2.08.402 2.599 1M12 8V7m0 1v8m0 0v1m0-1c-1.11 0-2.08-.402-2.599-1M21 12a9 9 0 11-18 0 9 9 0 0118 0z" />
                </svg>
              </div>
              <div>
                <h3 class="font-bold mb-2">Cost-Effective</h3>
                <p class="text-text-secondary text-sm">Factory-direct pricing offers 30-50% savings compared to branded alternatives.</p>
              </div>
            </div>
          </div>
        </div>
      </section>

      <!-- XH Product Range -->
      <section class="mb-16">
        <h2 class="text-2xl font-bold mb-6">XH Series Product Range</h2>
        <div class="grid grid-cols-1 md:grid-cols-3 gap-6">
          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold mb-3">XH Receptacle Housings</h3>
            <p class="text-text-secondary text-sm mb-4">Wire-side housings with positive lock mechanism.</p>
            <ul class="space-y-2 text-sm text-text-secondary">
              <li>• 2-15 positions available</li>
              <li>• Natural housing color</li>
              <li>• PA66 material</li>
              <li>• UL94V-0 flame rating</li>
            </ul>
          </div>
          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold mb-3">XH PCB Headers</h3>
            <p class="text-text-secondary text-sm mb-4">Through-hole and surface mount mounting options.</p>
            <ul class="space-y-2 text-sm text-text-secondary">
              <li>• THT and SMT available</li>
              <li>• Vertical and right-angle</li>
              <li>• Breakaway styles</li>
              <li>• Tin plating standard</li>
            </ul>
          </div>
          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold mb-3">XH Crimp Terminals</h3>
            <p class="text-text-secondary text-sm mb-4">Crimp terminals for wire termination.</p>
            <ul class="space-y-2 text-sm text-text-secondary">
              <li>• 26-22 AWG standard</li>
              <li>• Tin-plated copper</li>
              <li>• Bulk packaging</li>
              <li>• Reel packaging available</li>
            </ul>
          </div>
        </div>
      </section>

      <!-- Applications -->
      <section class="mb-16">
        <h2 class="text-2xl font-bold mb-6">Primary XH Series Applications</h2>
        <div class="space-y-6">
          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold text-lg mb-3 flex items-center gap-3">
              <span class="w-8 h-8 bg-accent-cyan/20 flex items-center justify-center text-accent-cyan text-sm font-mono">01</span>
              Industrial Control Systems
            </h3>
            <p class="text-text-secondary text-sm mb-4">PLC I/O connections, sensor wiring, actuator cables, and control panel interconnects. The positive lock ensures reliable connections in vibration environments.</p>
            <div class="flex gap-2">
              <span class="font-mono text-xs px-2 py-1 bg-accent-cyan/10 border border-accent-cyan/30 text-accent-cyan">Vibration Resistant</span>
              <span class="font-mono text-xs px-2 py-1 bg-accent-cyan/10 border border-accent-cyan/30 text-accent-cyan">Secure Lock</span>
            </div>
          </div>

          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold text-lg mb-3 flex items-center gap-3">
              <span class="w-8 h-8 bg-accent-cyan/20 flex items-center justify-center text-accent-cyan text-sm font-mono">02</span>
              LED Lighting & Power Supplies
            </h3>
            <p class="text-text-secondary text-sm mb-4">LED driver connections, power supply interfaces, and LED module wiring. 3A current capacity handles LED strip and module power requirements.</p>
            <div class="flex gap-2">
              <span class="font-mono text-xs px-2 py-1 bg-accent-cyan/10 border border-accent-cyan/30 text-accent-cyan">3A Rating</span>
              <span class="font-mono text-xs px-2 py-1 bg-accent-cyan/10 border border-accent-cyan/30 text-accent-cyan">250V</span>
            </div>
          </div>

          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold text-lg mb-3 flex items-center gap-3">
              <span class="w-8 h-8 bg-accent-cyan/20 flex items-center justify-center text-accent-cyan text-sm font-mono">03</span>
              Battery Management Systems
            </h3>
            <p class="text-text-secondary text-sm mb-4">Battery pack connections, BMS wiring, and power distribution in portable equipment and energy storage systems.</p>
            <div class="flex gap-2">
              <span class="font-mono text-xs px-2 py-1 bg-accent-cyan/10 border border-accent-cyan/30 text-accent-cyan">Battery Rated</span>
              <span class="font-mono text-xs px-2 py-1 bg-accent-cyan/10 border border-accent-cyan/30 text-accent-cyan">Secure Connection</span>
            </div>
          </div>

          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold text-lg mb-3 flex items-center gap-3">
              <span class="w-8 h-8 bg-accent-cyan/20 flex items-center justify-center text-accent-cyan text-sm font-mono">04</span>
              General Electronics
            </h3>
            <p class="text-text-secondary text-sm mb-4">Power supply connections, control boards, and general wire-to-board interconnects requiring reliable locking and moderate current capacity.</p>
            <div class="flex gap-2">
              <span class="font-mono text-xs px-2 py-1 bg-accent-cyan/10 border border-accent-cyan/30 text-accent-cyan">Versatile</span>
              <span class="font-mono text-xs px-2 py-1 bg-accent-cyan/10 border border-accent-cyan/30 text-accent-cyan">Widely Used</span>
            </div>
          </div>
        </div>
      </section>

      <!-- Custom Configurations -->
      <section class="mb-16">
        <h2 class="text-2xl font-bold mb-6">Custom XH Wire Harness Configurations</h2>
        <div class="grid grid-cols-1 md:grid-cols-2 gap-6">
          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold mb-3">Single-Ended Harnesses</h3>
            <p class="text-text-secondary text-sm mb-3">XH connector on one end, open wires or other connector on the opposite end.</p>
            <ul class="space-y-1 text-xs text-text-muted font-mono">
              <li>• Custom wire lengths</li>
              <li>• Various gauges</li>
              <li>• Color coding</li>
              <li>• Strain relief options</li>
            </ul>
          </div>
          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold mb-3">Double-Ended Harnesses</h3>
            <p class="text-text-secondary text-sm mb-3">XH connectors on both ends in same or different position counts.</p>
            <ul class="space-y-1 text-xs text-text-muted font-mono">
              <li>• XH-XH connections</li>
              <li>• Mixed position counts</li>
              <li>• Custom lengths</li>
              <li>• Shielding available</li>
            </ul>
          </div>
          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold mb-3">Multi-Branch Harnesses</h3>
            <p class="text-text-secondary text-sm mb-3">One XH connector splitting to multiple XH connectors or wire ends.</p>
            <ul class="space-y-1 text-xs text-text-muted font-mono">
              <li>• Power distribution</li>
              <li>• Signal routing</li>
              <li>• Custom breakouts</li>
              <li>• Labeling options</li>
            </ul>
          </div>
          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold mb-3">Shielded Assemblies</h3>
            <p class="text-text-secondary text-sm mb-3">XH connectors with shielded cable for EMI-sensitive applications.</p>
            <ul class="space-y-1 text-xs text-text-muted font-mono">
              <li>• Foil shielding</li>
              <li>• Braid shielding</li>
              <li>• Foil + braid</li>
              <li>• Drain wire</li>
            </ul>
          </div>
        </div>
      </section>

      <!-- Comparison -->
      <section class="mb-16">
        <h2 class="text-2xl font-bold mb-6">XH vs PH Series Comparison</h2>
        <div class="bg-bg-secondary border border-border p-6">
          <table class="w-full text-sm">
            <thead>
              <tr class="border-b border-border">
                <th class="text-left py-3 font-mono text-xs text-accent-cyan uppercase">Feature</th>
                <th class="text-center py-3 font-mono text-xs text-accent-cyan uppercase">PH Series</th>
                <th class="text-center py-3 font-mono text-xs text-accent-cyan uppercase">XH Series</th>
              </tr>
            </thead>
            <tbody class="text-text-secondary">
              <tr class="border-b border-border/50">
                <td class="py-3">Pitch</td>
                <td class="text-center py-3 font-mono">2.0mm</td>
                <td class="text-center py-3 font-mono">2.5mm</td>
              </tr>
              <tr class="border-b border-border/50">
                <td class="py-3">Current Rating</td>
                <td class="text-center py-3 font-mono">2A</td>
                <td class="text-center py-3 font-mono">3A</td>
              </tr>
              <tr class="border-b border-border/50">
                <td class="py-3">Voltage Rating</td>
                <td class="text-center py-3 font-mono">100V</td>
                <td class="text-center py-3 font-mono">250V</td>
              </tr>
              <tr class="border-b border-border/50">
                <td class="py-3">Locking</td>
                <td class="text-center py-3">Friction Lock</td>
                <td class="text-center py-3">Positive Lock</td>
              </tr>
              <tr>
                <td class="py-3">Primary Use</td>
                <td class="text-center py-3">Compact, Consumer</td>
                <td class="text-center py-3">Industrial, Power</td>
              </tr>
            </tbody>
          </table>
        </div>
      </section>

      <!-- CTA -->
      <section class="bg-bg-secondary border border-accent-cyan p-8 text-center">
        <h2 class="text-2xl font-bold mb-4">Need JST XH Compatible Wire Harness?</h2>
        <p class="text-text-secondary mb-6 max-w-2xl mx-auto">
          Share your XH series requirements for a competitive quote within 24 hours. Specify position count, wire length, and annual quantity.
        </p>
        <a href="/contact" class="inline-block px-8 py-4 font-mono text-sm font-bold uppercase tracking-widest text-bg-primary bg-accent-cyan hover:bg-accent-green transition-all duration-300">
          Get XH Series Quote
        </a>
      </section>
    </div>
  </section>
</MainLayout>
