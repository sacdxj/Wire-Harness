---
import MainLayout from '../../layouts/MainLayout.astro';
import Breadcrumb from '../../components/Breadcrumb.astro';

const breadcrumbItems = [
  { name: 'Home', href: '/' },
  { name: 'JST Compatible', href: '/compatibility/jst-compatible' }
];
---

<MainLayout
  title="JST Compatible Cable Assembly | Cost-Effective Alternative Solutions"
  description="JST compatible terminal wire assemblies at factory-direct prices. PH, XH, EH, VH, ZH series equivalents. Same quality, lower cost. 48-hour sampling, volume production."
>
  <script type="application/ld+json" set:html={JSON.stringify({
    "@context": "https://schema.org",
    "@type": "Product",
    "name": "JST Compatible Cable Assembly",
    "description": "Cost-effective JST-compatible wire harness assemblies for PH, XH, EH series connectors",
    "brand": {
      "@type": "Brand",
      "name": "Trycay"
    },
    "offers": {
      "@type": "Offer",
      "availability": "https://schema.org/InStock",
      "priceCurrency": "USD"
    }
  })} />

  <Breadcrumb items={breadcrumbItems} />

  <section class="px-4 py-16">
    <div class="max-w-5xl mx-auto">
      <div class="mb-12">
        <p class="font-mono text-xs text-accent-cyan uppercase tracking-widest mb-4">Connector Compatibility</p>
        <h1 class="text-4xl md:text-5xl font-bold tracking-tight mb-6">
          JST Compatible Cable Assembly</span>
        </h1>
        <p class="text-xl text-text-secondary leading-relaxed">
          Full range of JST-compatible terminal wire assemblies offering the same quality and reliability as genuine JST connectors at significantly lower cost. Direct from our ISO 9001 certified factory.
        </p>

        <!-- Disclaimer -->
        <div class="mb-8 p-4 bg-accent-amber/10 border border-accent-amber/30">
          <p class="font-mono text-xs text-accent-amber">
            <strong>Compatibility Notice:</strong> These are JST-compatible cable assemblies manufactured by Trycay.
            We are <strong>not affiliated with, authorized by, or using original JST components</strong>.
            All assemblies are produced to meet JST series specifications and are fully compatible with JST connectors.
          </p>
        </div>
      </div>

      <!-- Why Choose Compatible -->
      <section class="mb-16">
        <h2 class="text-2xl font-bold mb-6">Why Choose JST Compatible Solutions?</h2>
        <div class="grid grid-cols-1 md:grid-cols-3 gap-6">
          <div class="bg-bg-secondary border border-border p-6">
            <div class="font-mono text-3xl font-bold text-accent-green mb-2">30-50%</div>
            <div class="font-bold mb-2">Cost Savings</div>
            <p class="text-text-secondary text-sm">Significant cost reduction without compromising quality or performance.</p>
          </div>
          <div class="bg-bg-secondary border border-border p-6">
            <div class="font-mono text-3xl font-bold text-accent-green mb-2">100%</div>
            <div class="font-bold mb-2">Compatibility</div>
            <p class="text-text-secondary text-sm">Fully compatible with existing JST housings, headers, and applications.</p>
          </div>
          <div class="bg-bg-secondary border border-border p-6">
            <div class="font-mono text-3xl font-bold text-accent-green mb-2">48h</div>
            <div class="font-bold mb-2">Quick Delivery</div>
            <p class="text-text-secondary text-sm">Rapid prototyping and production from our dedicated manufacturing facility.</p>
          </div>
        </div>
      </section>

      <!-- JST Series Overview -->
      <section class="mb-16">
        <h2 class="text-2xl font-bold mb-6">Available JST Series Equivalents</h2>
        <div class="space-y-6">
          <!-- PH Series -->
          <div class="bg-bg-secondary border border-border p-6">
            <div class="flex items-start gap-6">
              <div class="w-16 h-16 bg-accent-cyan/10 border border-accent-cyan/30 flex items-center justify-center flex-shrink-0">
                <span class="font-mono text-2xl font-bold text-accent-cyan">PH</span>
              </div>
              <div class="flex-1">
                <h3 class="text-xl font-bold mb-2">PH Series - 2.0mm Pitch</h3>
                <p class="text-text-secondary text-sm mb-4">Ultra-compact wire-to-board connectors for high-density applications. Friction lock design ensures secure connections.</p>
                <div class="grid grid-cols-2 md:grid-cols-4 gap-4 mb-4">
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Pitch</div>
                    <div class="font-bold">2.0mm</div>
                  </div>
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Positions</div>
                    <div class="font-bold">2-15</div>
                  </div>
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Voltage</div>
                    <div class="font-bold">100V</div>
                  </div>
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Current</div>
                    <div class="font-bold">2A</div>
                  </div>
                </div>
                <div class="flex gap-2">
                  <a href="/compatibility/jst-ph-cable-assembly" class="font-mono text-xs text-accent-cyan hover:text-accent-green transition">View PH Series →</a>
                </div>
              </div>
            </div>
          </div>

          <!-- XH Series -->
          <div class="bg-bg-secondary border border-border p-6">
            <div class="flex items-start gap-6">
              <div class="w-16 h-16 bg-accent-cyan/10 border border-accent-cyan/30 flex items-center justify-center flex-shrink-0">
                <span class="font-mono text-2xl font-bold text-accent-cyan">XH</span>
              </div>
              <div class="flex-1">
                <h3 class="text-xl font-bold mb-2">XH Series - 2.5mm Pitch</h3>
                <p class="text-text-secondary text-sm mb-4">Positive locking mechanism for secure connections. Widely used in industrial and LED applications.</p>
                <div class="grid grid-cols-2 md:grid-cols-4 gap-4 mb-4">
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Pitch</div>
                    <div class="font-bold">2.5mm</div>
                  </div>
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Positions</div>
                    <div class="font-bold">2-15</div>
                  </div>
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Voltage</div>
                    <div class="font-bold">250V</div>
                  </div>
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Current</div>
                    <div class="font-bold">3A</div>
                  </div>
                </div>
                <div class="flex gap-2">
                  <a href="/compatibility/jst-xh-cable-assembly" class="font-mono text-xs text-accent-cyan hover:text-accent-green transition">View XH Series →</a>
                </div>
              </div>
            </div>
          </div>

          <!-- EH Series -->
          <div class="bg-bg-secondary border border-border p-6">
            <div class="flex items-start gap-6">
              <div class="w-16 h-16 bg-accent-cyan/10 border border-accent-cyan/30 flex items-center justify-center flex-shrink-0">
                <span class="font-mono text-2xl font-bold text-accent-cyan">EH</span>
              </div>
              <div class="flex-1">
                <h3 class="text-xl font-bold mb-2">EH Series - 2.5mm Pitch with Ejector</h3>
                <p class="text-text-secondary text-sm mb-4">Ejector design enables tool-free disconnect. Ideal for applications requiring frequent maintenance.</p>
                <div class="grid grid-cols-2 md:grid-cols-4 gap-4 mb-4">
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Pitch</div>
                    <div class="font-bold">2.5mm</div>
                  </div>
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Positions</div>
                    <div class="font-bold">2-15</div>
                  </div>
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Voltage</div>
                    <div class="font-bold">250V</div>
                  </div>
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Current</div>
                    <div class="font-bold">3A</div>
                  </div>
                </div>
                <div class="flex gap-2">
                  <span class="font-mono text-xs px-2 py-1 bg-accent-amber/10 border border-accent-amber/30 text-accent-amber">Tool-Free Disconnect</span>
                </div>
              </div>
            </div>
          </div>

          <!-- VH Series -->
          <div class="bg-bg-secondary border border-border p-6">
            <div class="flex items-start gap-6">
              <div class="w-16 h-16 bg-accent-cyan/10 border border-accent-cyan/30 flex items-center justify-center flex-shrink-0">
                <span class="font-mono text-2xl font-bold text-accent-cyan">VH</span>
              </div>
              <div class="flex-1">
                <h3 class="text-xl font-bold mb-2">VH Series - 3.96mm Pitch</h3>
                <p class="text-text-secondary text-sm mb-4">Higher power capacity connectors for power delivery applications. Robust locking mechanism.</p>
                <div class="grid grid-cols-2 md:grid-cols-4 gap-4 mb-4">
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Pitch</div>
                    <div class="font-bold">3.96mm</div>
                  </div>
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Positions</div>
                    <div class="font-bold">2-12</div>
                  </div>
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Voltage</div>
                    <div class="font-bold">250V</div>
                  </div>
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Current</div>
                    <div class="font-bold">7A</div>
                  </div>
                </div>
                <div class="flex gap-2">
                  <span class="font-mono text-xs px-2 py-1 bg-accent-amber/10 border border-accent-amber/30 text-accent-amber">High Current</span>
                </div>
              </div>
            </div>
          </div>

          <!-- ZH Series -->
          <div class="bg-bg-secondary border border-border p-6">
            <div class="flex items-start gap-6">
              <div class="w-16 h-16 bg-accent-cyan/10 border border-accent-cyan/30 flex items-center justify-center flex-shrink-0">
                <span class="font-mono text-2xl font-bold text-accent-cyan">ZH</span>
              </div>
              <div class="flex-1">
                <h3 class="text-xl font-bold mb-2">ZH Series - 1.5mm Miniature Pitch</h3>
                <p class="text-text-secondary text-sm mb-4">Ultra-fine pitch for compact applications. Positive locking ensures reliable connections.</p>
                <div class="grid grid-cols-2 md:grid-cols-4 gap-4 mb-4">
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Pitch</div>
                    <div class="font-bold">1.5mm</div>
                  </div>
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Positions</div>
                    <div class="font-bold">2-15</div>
                  </div>
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Voltage</div>
                    <div class="font-bold">100V</div>
                  </div>
                  <div>
                    <div class="font-mono text-xs text-text-muted uppercase">Current</div>
                    <div class="font-bold">1A</div>
                  </div>
                </div>
                <div class="flex gap-2">
                  <span class="font-mono text-xs px-2 py-1 bg-accent-amber/10 border border-accent-amber/30 text-accent-amber">Ultra-Compact</span>
                </div>
              </div>
            </div>
          </div>
        </div>
      </section>

      <!-- Quality Assurance -->
      <section class="mb-16">
        <h2 class="text-2xl font-bold mb-6">Quality You Can Trust</h2>
        <div class="grid grid-cols-1 md:grid-cols-2 gap-6">
          <div class="border-l-2 border-accent-cyan pl-6">
            <h3 class="font-bold mb-2">Dimensional Compatibility</h3>
            <p class="text-text-secondary text-sm">Precise dimensional control ensures our compatible connectors mate perfectly with existing JST housings and headers.</p>
          </div>
          <div class="border-l-2 border-accent-cyan pl-6">
            <h3 class="font-bold mb-2">Material Quality</h3>
            <p class="text-text-secondary text-sm">PA66, PBT, and other high-quality materials with UL94V-0 flame rating for safety and reliability.</p>
          </div>
          <div class="border-l-2 border-accent-cyan pl-6">
            <h3 class="font-bold mb-2">Electrical Performance</h3>
            <p class="text-text-secondary text-sm">Current and voltage ratings meet or exceed JST specifications for reliable power delivery.</p>
          </div>
          <div class="border-l-2 border-accent-cyan pl-6">
            <h3 class="font-bold mb-2">Testing & Validation</h3>
            <p class="text-text-secondary text-sm">100% continuity testing, pull testing, and inspection ensures consistent quality.</p>
          </div>
        </div>
      </section>

      <!-- Product Range -->
      <section class="mb-16">
        <h2 class="text-2xl font-bold mb-6">Complete Product Range</h2>
        <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-6">
          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold mb-3">Receptacle Housings</h3>
            <p class="text-text-secondary text-sm mb-3">Wire-side housings with friction lock or positive lock mechanisms.</p>
            <ul class="space-y-1 text-xs text-text-muted font-mono">
              <li>• PH/XH/EH/VH/ZH series</li>
              <li>• 2-15 positions</li>
              <li>• Various color options</li>
            </ul>
          </div>
          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold mb-3">PCB Headers</h3>
            <p class="text-text-secondary text-sm mb-3">Through-hole and surface mount headers for PCB mounting.</p>
            <ul class="space-y-1 text-xs text-text-muted font-mono">
              <li>• THT and SMT options</li>
              <li>• Vertical and right-angle</li>
              <li>• Breakaway styles available</li>
            </ul>
          </div>
          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold mb-3">Crimp Terminals</h3>
            <p class="text-text-secondary text-sm mb-3">Tin-plated copper terminals for reliable wire connections.</p>
            <ul class="space-y-1 text-xs text-text-muted font-mono">
              <li>• Various wire gauge ranges</li>
              <li>• Bulk and reel packaging</li>
              <li>• Crimp tooling available</li>
            </ul>
          </div>
        </div>
      </section>

      <!-- Applications -->
      <section class="mb-16">
        <h2 class="text-2xl font-bold mb-6">Applications Using JST Compatible Connectors</h2>
        <div class="flex flex-wrap gap-3">
          <span class="font-mono text-xs px-4 py-2 bg-bg-secondary border border-border text-text-secondary">Consumer Electronics</span>
          <span class="font-mono text-xs px-4 py-2 bg-bg-secondary border border-border text-text-secondary">LED Lighting</span>
          <span class="font-mono text-xs px-4 py-2 bg-bg-secondary border border-border text-text-secondary">Industrial Controls</span>
          <span class="font-mono text-xs px-4 py-2 bg-bg-secondary border border-border text-text-secondary">Automotive Electronics</span>
          <span class="font-mono text-xs px-4 py-2 bg-bg-secondary border border-border text-text-secondary">Power Supplies</span>
          <span class="font-mono text-xs px-4 py-2 bg-bg-secondary border border-border text-text-secondary">Battery Management</span>
          <span class="font-mono text-xs px-4 py-2 bg-bg-secondary border border-border text-text-secondary">IoT Devices</span>
          <span class="font-mono text-xs px-4 py-2 bg-bg-secondary border border-border text-text-secondary">Medical Equipment</span>
        </div>
      </section>

      <!-- CTA -->
      <section class="bg-bg-secondary border border-accent-cyan p-8 text-center">
        <h2 class="text-2xl font-bold mb-4">Ready to Save on JST Compatible Cable Assembly?</h2>
        <p class="text-text-secondary mb-6 max-w-2xl mx-auto">
          Get a competitive quote within 24 hours. Share your JST part numbers or specifications for cross-reference and pricing.
        </p>
        <a href="/contact" class="inline-block px-8 py-4 font-mono text-sm font-bold uppercase tracking-widest text-bg-primary bg-accent-cyan hover:bg-accent-green transition-all duration-300">
          Request JST Compatible Quote
        </a>
      </section>
    </div>
  </section>
</MainLayout>
