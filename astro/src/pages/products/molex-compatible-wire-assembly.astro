    },
    "manufacturer": {
      "@type": "Organization",
      "name": "Trycay"
    },
    "offers": {
      "@type": "Offer",
      "availability": "https://schema.org/InStock",
      "priceCurrency": "USD",
      "minimumOrderQuantity": "100"
