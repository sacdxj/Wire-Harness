---
import MainLayout from '../../layouts/MainLayout.astro';
import Breadcrumb from '../../components/Breadcrumb.astro';

const breadcrumbItems = [
  { name: 'Home', href: '/' },
  { name: 'Products', href: '/products' },
  { name: 'PH Series Crimp Terminal', href: '/products/ph-terminal' }
];

const product = {
  code: 'SFR-201',
  series: 'PH',
  name: 'PH Series Crimp Terminal Contact',
  type: 'Crimp Terminal',
  wireGauge: '28-24 AWG',
  material: 'Tin-Plated Copper',
  finish: 'Tin',
  packaging: 'Reel',
  plating: 'Tin over Copper'
};
---

<MainLayout
  title="PH Series Crimp Terminal | SFR-201 JST Compatible"
  description="Tin-plated copper crimp terminal for PH Series 2.0mm pitch connectors. 28-24 AWG wire gauge. Compatible with JST PH series terminals and applicators."
  image="/images/products/ph-series-crimp-terminal.jpg"
>
  <script type="application/ld+json" set:html={JSON.stringify({
    "@context": "https://schema.org",
    "@type": "Product",
    "name": "PH Series Crimp Terminal Contact",
    "model": "SFR-201",
    "description": "Tin-plated copper crimp terminal for 28-24 AWG wires compatible with JST PH series",
    "brand": {
      "@type": "Brand",
      "name": "Trycay"
    },
    "offers": {
      "@type": "Offer",
      "availability": "https://schema.org/InStock",
      "priceCurrency": "USD"
    },
    "properties": {
      "wireGauge": "28-24 AWG",
      "material": "Tin-plated copper",
      "plating": "Tin"
    }
  })} />

  <Breadcrumb items={breadcrumbItems} />

  <section class="px-4 py-16">
    <div class="max-w-5xl mx-auto">
      <!-- Header -->
      <div class="mb-12">
        <div class="flex flex-wrap items-center gap-4 mb-4">
          <span class="font-mono text-xs text-accent-cyan uppercase tracking-widest">PH Series</span>
          <span class="font-mono text-xs text-accent-amber uppercase tracking-wider px-2 py-1 bg-accent-amber/10 border border-accent-amber/30">
            {product.code}
          </span>
          <span class="flex items-center gap-2 font-mono text-xs text-text-muted">
            <span class="w-2 h-2 rounded-full bg-accent-green"></span>
            In Stock
          </span>
        </div>
        <h1 class="text-4xl md:text-5xl font-bold tracking-tight mb-6">
          {product.name}
        </h1>
        <p class="text-xl text-text-secondary leading-relaxed">
          High-quality tin-plated copper crimp terminals designed for PH Series 2.0mm pitch connectors. Precision-engineered for reliable wire termination and optimal electrical performance with 28-24 AWG wires.
        </p>
      </div>

      <!-- Product Image -->
      <div class="mb-12 bg-bg-secondary border border-border rounded-lg overflow-hidden">
        <img
          src="/images/products/ph-series-crimp-terminal.jpg"
          alt="PH Series Crimp Terminal and Connector System - SFR-201"
          class="w-full h-auto"
          loading="eager"
        />
      </div>

      <!-- Quick Specs -->
      <section class="mb-16">
        <div class="bg-bg-secondary border border-accent-cyan p-8">
          <h2 class="text-2xl font-bold mb-6">Quick Specifications</h2>
          <div class="grid grid-cols-2 md:grid-cols-4 gap-6">
            <div>
              <div class="font-mono text-xs text-text-muted uppercase tracking-wider mb-1">Part Number</div>
              <div class="font-mono text-xl font-bold">{product.code}</div>
            </div>
            <div>
              <div class="font-mono text-xs text-text-muted uppercase tracking-wider mb-1">Wire Gauge</div>
              <div class="font-mono text-2xl font-bold text-accent-cyan">{product.wireGauge}</div>
            </div>
            <div>
              <div class="font-mono text-xs text-text-muted uppercase tracking-wider mb-1">Material</div>
              <div class="font-mono text-2xl font-bold text-accent-cyan">Cu+Sn</div>
            </div>
            <div>
              <div class="font-mono text-xs text-text-muted uppercase tracking-wider mb-1">Packaging</div>
              <div class="font-mono text-2xl font-bold text-accent-cyan">Reel</div>
            </div>
          </div>
        </div>
      </section>

      <!-- Detailed Specifications -->
      <section class="mb-16">
        <h2 class="text-2xl font-bold mb-6">Technical Specifications</h2>
        <div class="grid grid-cols-1 md:grid-cols-2 gap-6">
          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold text-lg mb-4">Electrical Characteristics</h3>
            <table class="w-full text-sm">
              <tbody class="text-text-secondary">
                <tr class="border-b border-border/50">
                  <td class="py-2 font-medium">Current Rating</td>
                  <td class="py-2 font-mono">2A max</td>
                </tr>
                <tr class="border-b border-border/50">
                  <td class="py-2 font-medium">Voltage Rating</td>
                  <td class="py-2 font-mono">100V AC/DC</td>
                </tr>
                <tr class="border-b border-border/50">
                  <td class="py-2 font-medium">Contact Resistance</td>
                  <td class="py-2 font-mono">10 mΩ max</td>
                </tr>
                <tr>
                  <td class="py-2 font-medium">Withstanding Voltage</td>
                  <td class="py-2 font-mono">500V AC</td>
                </tr>
              </tbody>
            </table>
          </div>

          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold text-lg mb-4">Mechanical Specifications</h3>
            <table class="w-full text-sm">
              <tbody class="text-text-secondary">
                <tr class="border-b border-border/50">
                  <td class="py-2 font-medium">Applicable Wire Gauge</td>
                  <td class="py-2 font-mono">{product.wireGauge}</td>
                </tr>
                <tr class="border-b border-border/50">
                  <td class="py-2 font-medium">Terminal Material</td>
                  <td class="py-2 font-mono">{product.material}</td>
                </tr>
                <tr class="border-b border-border/50">
                  <td class="py-2 font-medium">Plating Material</td>
                  <td class="py-2 font-mono">{product.finish}</td>
                </tr>
                <tr>
                  <td class="py-2 font-medium">Packaging</td>
                  <td class="py-2 font-mono">{product.packaging} (1000 pcs)</td>
                </tr>
              </tbody>
            </table>
          </div>
        </div>
      </section>

      <!-- Crimp Dimensions -->
      <section class="mb-16">
        <h2 class="text-2xl font-bold mb-6">Crimp Dimensions & Requirements</h2>
        <div class="bg-bg-secondary border border-border p-6">
          <table class="w-full text-sm">
            <thead>
              <tr class="border-b border-border">
                <th class="text-left py-3 font-mono text-xs text-accent-cyan uppercase">Parameter</th>
                <th class="text-left py-3 font-mono text-xs text-accent-cyan uppercase">Specification</th>
              </tr>
            </thead>
            <tbody class="text-text-secondary">
              <tr class="border-b border-border/50">
                <td class="py-3 font-medium">Wire Insulation OD</td>
                <td class="py-3 font-mono">0.7 - 1.0mm</td>
              </tr>
              <tr class="border-b border-border/50">
                <td class="py-3 font-medium">Conductor Size</td>
                <td class="py-3 font-mono">0.08 - 0.20mm²</td>
              </tr>
              <tr class="border-b border-border/50">
                <td class="py-3 font-medium">Crimp Height (Insulation)</td>
                <td class="py-3 font-mono">0.80 - 1.00mm</td>
              </tr>
              <tr class="border-b border-border/50">
                <td class="py-3 font-medium">Crimp Height (Conductor)</td>
                <td class="py-3 font-mono">0.40 - 0.60mm</td>
              </tr>
              <tr>
                <td class="py-3 font-medium">Recommended Crimp Tool</td>
                <td class="py-3 font-mono">PH Hand Crimp Tool / Applicator</td>
              </tr>
            </tbody>
          </table>
        </div>
      </section>

      <!-- Features -->
      <section class="mb-16">
        <h2 class="text-2xl font-bold mb-6">Key Features</h2>
        <div class="grid grid-cols-1 md:grid-cols-2 gap-6">
          <div class="flex gap-4">
            <div class="w-12 h-12 flex-shrink-0 bg-accent-cyan/10 border border-accent-cyan/30 flex items-center justify-center text-accent-cyan">
              <svg class="w-6 h-6" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z" />
              </svg>
            </div>
            <div>
              <h3 class="font-bold mb-2">High Conductivity</h3>
              <p class="text-text-secondary text-sm">Pure copper base material ensures excellent electrical conductivity and current carrying capacity.</p>
            </div>
          </div>
          <div class="flex gap-4">
            <div class="w-12 h-12 flex-shrink-0 bg-accent-cyan/10 border border-accent-cyan/30 flex items-center justify-center text-accent-cyan">
              <svg class="w-6 h-6" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" d="M7 21a4 4 0 01-4-4V5a2 2 0 012-2h4a2 2 0 012 2v12a4 4 0 01-4 4zm0 0h12a2 2 0 002-2v-4a2 2 0 00-2-2h-2.343M11 7.343l1.657-1.657a2 2 0 012.828 0l2.829 2.829a2 2 0 010 2.828l-8.486 8.485M7 17h.01" />
              </svg>
            </div>
            <div>
              <h3 class="font-bold mb-2">Tin Plating</h3>
              <p class="text-text-secondary text-sm">Tin plating provides corrosion resistance and ensures reliable crimp connections.</p>
            </div>
          </div>
          <div class="flex gap-4">
            <div class="w-12 h-12 flex-shrink-0 bg-accent-cyan/10 border border-accent-cyan/30 flex items-center justify-center text-accent-cyan">
              <svg class="w-6 h-6" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" d="M19.428 15.428a2 2 0 00-1.022-.547l-2.387-.477a6 6 0 00-3.86.517l-.318.158a6 6 0 01-3.86.517L6.05 15.21a2 2 0 00-1.806.547M8 4h8l-1 1v5.172a2 2 0 00.586 1.414l5 5c1.26 1.26.367 3.414-1.415 3.414H4.828c-1.782 0-2.674-2.154-1.414-3.414l5-5A2 2 0 009 10.172V5L8 4z" />
              </svg>
            </div>
            <div>
              <h3 class="font-bold mb-2">Precision Crimp Design</h3>
              <p class="text-text-secondary text-sm">Engineered crimp zones for secure wire grip and optimal electrical contact.</p>
            </div>
          </div>
          <div class="flex gap-4">
            <div class="w-12 h-12 flex-shrink-0 bg-accent-cyan/10 border border-accent-cyan/30 flex items-center justify-center text-accent-cyan">
              <svg class="w-6 h-6" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" d="M4 5a1 1 0 011-1h14a1 1 0 011 1v2a1 1 0 01-1 1H5a1 1 0 01-1-1V5zM4 13a1 1 0 011-1h6a1 1 0 011 1v6a1 1 0 01-1 1H5a1 1 0 01-1-1v-6zM16 13a1 1 0 011-1h2a1 1 0 011 1v6a1 1 0 01-1 1h-2a1 1 0 01-1-1v-6z" />
              </svg>
            </div>
            <div>
              <h3 class="font-bold mb-2">JST PH Compatible</h3>
              <p class="text-text-secondary text-sm">100% compatible with JST PH series applicators and receptacle housings.</p>
            </div>
          </div>
        </div>
      </section>

      <!-- Applications -->
      <section class="mb-16">
        <h2 class="text-2xl font-bold mb-6">Typical Applications</h2>
        <div class="grid grid-cols-1 md:grid-cols-3 gap-6">
          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold mb-2">Wire Harness Assembly</h3>
            <p class="text-text-secondary text-sm">Crimping to wires for assembly into PH series connector housings.</p>
          </div>
          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold mb-2">Prototype Development</h3>
            <p class="text-text-secondary text-sm">Hand-crimping for low-volume prototyping and testing.</p>
          </div>
          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold mb-2">Volume Production</h3>
            <p class="text-text-secondary text-sm">Reel packaging for automated crimp machine applications.</p>
          </div>
          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold mb-2">Field Service</h3>
            <p class="text-text-secondary text-sm">Repair and maintenance of existing wire harness assemblies.</p>
          </div>
          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold mb-2">Custom Cable Assemblies</h3>
            <p class="text-text-secondary text-sm">Building custom cable assemblies with specific requirements.</p>
          </div>
          <div class="bg-bg-secondary border border-border p-6">
            <h3 class="font-bold mb-2">OEM Manufacturing</h3>
            <p class="text-text-secondary text-sm">Integration into consumer electronics and industrial products.</p>
          </div>
        </div>
      </section>

      <!-- Compatible Products -->
      <section class="mb-16">
        <h2 class="text-2xl font-bold mb-6">Related Products</h2>
        <div class="grid grid-cols-1 md:grid-cols-3 gap-6">
          <a href="/products/ph-series-2pin" class="group block bg-bg-secondary border border-border p-6 transition-all duration-300 hover:border-accent-cyan">
            <span class="font-mono text-xs text-accent-amber uppercase">SFR-001</span>
            <h3 class="font-bold mt-2 mb-1 group-hover:text-accent-cyan">PH Series 2-Pin Housing</h3>
            <p class="text-text-secondary text-sm">2-position housing for these terminals</p>
          </a>
          <a href="/products/ph-series-4pin" class="group block bg-bg-secondary border border-border p-6 transition-all duration-300 hover:border-accent-cyan">
            <span class="font-mono text-xs text-accent-amber uppercase">SFR-004</span>
            <h3 class="font-bold mt-2 mb-1 group-hover:text-accent-cyan">PH Series 4-Pin Housing</h3>
            <p class="text-text-secondary text-sm">4-position housing option</p>
          </a>
          <a href="/compatibility/jst-ph-cable-assembly" class="group block bg-bg-secondary border border-border p-6 transition-all duration-300 hover:border-accent-cyan">
            <span class="font-mono text-xs text-accent-cyan uppercase">PH Assembly</span>
            <h3 class="font-bold mt-2 mb-1 group-hover:text-accent-cyan">Complete PH Assemblies</h3>
            <p class="text-text-secondary text-sm">Ready-to-use cable assemblies</p>
          </a>
        </div>
      </section>

      <!-- CTA -->
      <section class="bg-bg-secondary border border-accent-cyan p-8 text-center">
        <h2 class="text-2xl font-bold mb-4">Request Quote for SFR-201</h2>
        <p class="text-text-secondary mb-6 max-w-2xl mx-auto">
          Available in bulk and reel packaging. Specify quantity and packaging preference for volume pricing within 24 hours.
        </p>
        <a href="/contact" class="inline-block px-8 py-4 font-mono text-sm font-bold uppercase tracking-widest text-bg-primary bg-accent-cyan hover:bg-accent-green transition-all duration-300">
          Get Quote
        </a>
      </section>
    </div>
  </section>
</MainLayout>
