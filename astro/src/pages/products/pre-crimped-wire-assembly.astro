---
import MainLayout from '../../layouts/MainLayout.astro';
import Breadcrumb from '../../components/Breadcrumb.astro';

const breadcrumbItems = [
  { name: 'Home', href: '/' },
  { name: 'Products', href: '/products' },
  { name: 'Pre-crimped Wire Assembly', href: '/products/pre-crimped-wire-assembly' }
];

const wireOptions = [
  { type: 'UL1007', awg: '28-20', temp: '80¬∞C', voltage: '300V', description: 'Standard internal wiring' },
  { type: 'UL1015', awg: '26-16', temp: '105¬∞C', voltage: '600V', description: 'High-temperature applications' },
  { type: 'UL1061', awg: '30-22', temp: '80¬∞C', voltage: '300V', description: 'Compact electronic connections' },
  { type: 'Silicone', awg: '26-14', temp: '150¬∞C', voltage: '600V', description: 'High-flex, medical/industrial' },
  { type: 'Teflon', awg: '28-20', temp: '200¬∞C', voltage: '600V', description: 'Extreme temperature environments' }
];

const connectorSeries = [
  { brand: 'JST Compatible', series: 'PH (2.0mm), XH (2.5mm), SH (1.0mm), GH (1.25mm), VH (3.96mm)', pitch: '1.0-3.96mm' },
  { brand: 'Molex Compatible', series: 'KK 254, Micro-Fit 3.0, PicoBlade', pitch: '1.25-3.0mm' },
  { brand: 'DuPont Compatible', series: '2.54mm standard', pitch: '2.54mm' },
  { brand: 'Custom Terminals', series: 'Per your specifications', pitch: 'Any' }
];

const qualityTests = [
  { name: 'Continuity Test', desc: '100% electrical continuity verification', icon: '‚ö°' },
  { name: 'Pull Test', desc: 'Crimp pull-off force: 5-12N minimum', icon: 'üí™' },
  { name: 'Dimensional Check', desc: 'Crimp height tolerance: ¬±0.02mm', icon: 'üìè' },
  { name: 'Visual Inspection', desc: '20x microscope inspection', icon: 'üî¨' },
  { name: 'Insulation Resistance', desc: '1000 MŒ© minimum at 500V DC', icon: 'üîí' },
  { name: 'Withstand Voltage', desc: '500-600V AC for 1 minute', icon: '‚ö°' }
];

const customSpecs = [
  { label: 'Wire Length', range: '10mm - 5 meters', unit: 'Custom lengths' },
  { label: 'Wire Gauge', range: 'AWG 32 - 14', unit: 'UL certified' },
  { label: 'Terminal Type', range: 'Custom', unit: 'JST/Molex/DuPont/Spec' },
  { label: 'Crimp Config', range: 'Single/Double', unit: 'One or both ends' },
  { label: 'Strip Length', range: 'Custom', unit: 'Per specification' },
  { label: 'MOQ', range: '100 pcs', unit: 'Prototypes welcome' }
];
---

<MainLayout
  title="Pre-Crimped Wire Assembly | Custom Crimped Wire Leads Manufacturer"
  description="Factory-direct pre-crimped wire assemblies and custom crimped wire leads. Single/dual-end crimped terminals. JST, Molex, DuPont compatible. UL certified wire, AWG 32-14, 100% tested."
>
  <script type="application/ld+json" set:html={JSON.stringify({
    "@context": "https://schema.org",
    "@type": "Product",
    "name": "Pre-Crimped Wire Assembly",
    "description": "Custom pre-crimped wire assemblies and crimped wire leads manufactured to specifications",
    "brand": {
      "@type": "Brand",
      "name": "Trycay"
    },
    "manufacturer": {
      "@type": "Organization",
      "name": "Trycay"
    },
    "offers": {
      "@type": "Offer",
      "availability": "https://schema.org/InStock",
      "priceCurrency": "USD",
      "minimumOrderQuantity": "100"
    },
    "additionalProperty": [
      {
        "@type": "PropertyValue",
        "name": "Wire Gauge",
        "value": "AWG 32-14"
      },
      {
        "@type": "PropertyValue",
        "name": "Wire Types",
        "value": "UL1007, UL1015, Silicone, Teflon"
      },
      {
        "@type": "PropertyValue",
        "name": "Terminal Compatibility",
        "value": "JST, Molex, DuPont compatible"
      },
      {
        "@type": "PropertyValue",
        "name": "Crimp Configuration",
        "value": "Single-end or dual-end"
      }
    ]
  })} />

  <Breadcrumb items={breadcrumbItems} />

  <!-- Hero Section -->
  <section class="px-4 py-16 bg-bg-secondary border-b border-border">
    <div class="max-w-7xl mx-auto">
      <div class="grid grid-cols-1 lg:grid-cols-2 gap-12 items-center">
        <div>
          <p class="font-mono text-xs text-accent-cyan uppercase tracking-widest mb-4">Pre-Crimped Wire Assembly</p>
          <h1 class="text-4xl md:text-5xl font-bold tracking-tight mb-6">
            Custom Pre-Crimped Wire Leads
          </h1>
          <p class="text-lg text-text-secondary mb-8">
            Factory-direct pre-crimped wire assemblies manufactured to your specifications.
            Single-end or dual-end crimped terminals with custom wire gauge, length, and connector compatibility.
            UL-certified wire, 100% electrical testing, and ISO-compliant quality control.
          </p>

          <div class="flex flex-wrap gap-3 mb-8">
            <div class="px-4 py-2 bg-accent-green/10 border border-accent-green/30">
              <span class="font-mono text-xs text-accent-green">‚úì UL Certified Wire</span>
            </div>
            <div class="px-4 py-2 bg-accent-green/10 border border-accent-green/30">
              <span class="font-mono text-xs text-accent-green">‚úì 100% Tested</span>
            </div>
            <div class="px-4 py-2 bg-accent-green/10 border border-accent-green/30">
              <span class="font-mono text-xs text-accent-green">‚úì AWG 32-14</span>
            </div>
            <div class="px-4 py-2 bg-accent-green/10 border border-accent-green/30">
              <span class="font-mono text-xs text-accent-green">‚úì MOQ 100 pcs</span>
            </div>
          </div>

          <div class="flex flex-wrap gap-4">
            <a href="/contact" class="inline-block px-8 py-4 font-mono text-sm font-bold uppercase tracking-widest text-bg-primary bg-accent-cyan hover:bg-accent-green transition-all duration-300">
              Upload Drawing for Quote
            </a>
            <a href="/oem-wire-cable-assembly-manufacturer" class="inline-block px-8 py-4 font-mono text-sm font-bold uppercase tracking-widest text-accent-cyan border border-accent-cyan hover:bg-accent-cyan/10 transition-all duration-300">
              OEM Services
            </a>
          </div>
        </div>

        <div class="bg-bg-tertiary border border-border p-8">
          <h2 class="font-mono text-lg font-bold text-accent-cyan mb-6">Connector Compatibility</h2>
          <div class="space-y-4">
            {connectorSeries.map(conn => (
              <div class="border border-border p-4">
                <div class="font-bold text-accent-cyan mb-1">{conn.brand}</div>
                <div class="text-sm text-text-secondary mb-2">{conn.series}</div>
                <div class="font-mono text-xs text-text-muted">Pitch: {conn.pitch}</div>
              </div>
            ))}
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- Applications -->
  <section class="px-4 py-16">
    <div class="max-w-7xl mx-auto">
      <div class="mb-12">
        <p class="font-mono text-xs text-accent-cyan uppercase tracking-widest mb-3">What We Crimp</p>
        <h2 class="text-3xl font-bold tracking-tight">Pre-Crimped Wire Applications</h2>
        <p class="text-text-secondary mt-4 max-w-3xl">
          Pre-crimped wires are used when you need ready-to-assemble wire leads with terminals already attached.
          Ideal for prototyping, mass production, and field installation.
        </p>
      </div>

      <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-6">
        <div class="border border-border p-6">
          <h3 class="font-bold text-lg mb-3">Prototyping & Development</h3>
          <p class="text-text-secondary text-sm">
            Rapid iteration with pre-terminated wires. Test circuits and configurations without manual crimping.
          </p>
        </div>

        <div class="border border-border p-6">
          <h3 class="font-bold text-lg mb-3">Mass Production Assembly</h3>
          <p class="text-text-secondary text-sm">
            Speed up production lines with ready-to-install wire leads. Consistent quality, reduced labor.
          </p>
        </div>

        <div class="border border-border p-6">
          <h3 class="font-bold text-lg mb-3">Field Installation</h3>
          <p class="text-text-secondary text-sm">
            Pre-crimped leads for on-site assembly. No crimping tools required at installation point.
          </p>
        </div>

        <div class="border border-border p-6">
          <h3 class="font-bold text-lg mb-3">Battery Connections</h3>
          <p class="text-text-secondary text-sm">
            Battery pack assembly with pre-crimped leads. Reliable connections for BMS and power distribution.
          </p>
        </div>

        <div class="border border-border p-6">
          <h3 class="font-bold text-lg mb-3">Sensor Integration</h3>
          <p class="text-text-secondary text-sm">
            Sensor cables with pre-crimped connectors. Quick connection to control systems and data acquisition.
          </p>
        </div>

        <div class="border border-border p-6">
          <h3 class="font-bold text-lg mb-3">LED Lighting</h3>
          <p class="text-text-secondary text-sm">
            LED strip connections with pre-crimped wires. Simplify LED driver and power supply installation.
          </p>
        </div>
      </div>
    </div>
  </section>

  <!-- Wire Options -->
  <section class="px-4 py-16 bg-bg-secondary border-y border-border">
    <div class="max-w-7xl mx-auto">
      <div class="mb-12">
        <p class="font-mono text-xs text-accent-cyan uppercase tracking-widest mb-3">Wire Specifications</p>
        <h2 class="text-3xl font-bold tracking-tight">Available Wire Types</h2>
        <p class="text-text-secondary mt-4 max-w-3xl">
          All wire options are UL-certified and selected based on current rating, temperature requirements,
          and application environment.
        </p>
      </div>

      <div class="overflow-x-auto">
        <table class="w-full">
          <thead>
            <tr class="border-b-2 border-border">
              <th class="px-4 py-4 text-left font-mono text-sm text-accent-cyan uppercase tracking-wider">Wire Type</th>
              <th class="px-4 py-4 text-left font-mono text-sm text-accent-cyan uppercase tracking-wider">AWG Range</th>
              <th class="px-4 py-4 text-left font-mono text-sm text-accent-cyan uppercase tracking-wider">Temp Rating</th>
              <th class="px-4 py-4 text-left font-mono text-sm text-accent-cyan uppercase tracking-wider">Voltage Rating</th>
              <th class="px-4 py-4 text-left font-mono text-sm text-accent-cyan uppercase tracking-wider">Description</th>
            </tr>
          </thead>
          <tbody>
            {wireOptions.map(wire => (
              <tr class="border-b border-border hover:bg-bg-tertiary">
                <td class="px-4 py-4 font-mono font-bold">{wire.type}</td>
                <td class="px-4 py-4 font-mono">AWG {wire.awg}</td>
                <td class="px-4 py-4 font-mono text-accent-cyan">{wire.temp}</td>
                <td class="px-4 py-4 font-mono">{wire.voltage}</td>
                <td class="px-4 py-4 text-text-secondary">{wire.description}</td>
              </tr>
            ))}
          </tbody>
        </table>
      </div>
    </div>
  </section>

  <!-- Crimp Configuration -->
  <section class="px-4 py-16">
    <div class="max-w-7xl mx-auto">
      <div class="mb-12">
        <p class="font-mono text-xs text-accent-cyan uppercase tracking-widest mb-3">Configuration Options</p>
        <h2 class="text-3xl font-bold tracking-tight">Crimp Specifications</h2>
        <p class="text-text-secondary mt-4 max-w-3xl">
          Choose from single-end or dual-end crimped configurations. Specify exact terminal types and wire specifications.
        </p>
      </div>

      <div class="grid grid-cols-1 md:grid-cols-2 gap-8 mb-12">
        <div class="border-2 border-accent-cyan p-8">
          <h3 class="font-bold text-2xl mb-4 text-accent-cyan">Single-End Crimped</h3>
          <p class="text-text-secondary mb-6">
            One end crimped with terminal, other end stripped or tinned. Ready for soldering or terminal block connection.
          </p>
          <ul class="space-y-2 text-text-secondary text-sm">
            <li class="flex items-start gap-2">
              <span class="text-accent-cyan">‚ñ∏</span>
              <span>Terminal on one end only</span>
            </li>
            <li class="flex items-start gap-2">
              <span class="text-accent-cyan">‚ñ∏</span>
              <span>Other end stripped or tinned</span>
            </li>
            <li class="flex items-start gap-2">
              <span class="text-accent-cyan">‚ñ∏</span>
              <span>Cost-effective for board connections</span>
            </li>
            <li class="flex items-start gap-2">
              <span class="text-accent-cyan">‚ñ∏</span>
              <span>Custom wire lengths</span>
            </li>
          </ul>
        </div>

        <div class="border-2 border-accent-green p-8">
          <h3 class="font-bold text-2xl mb-4 text-accent-green">Dual-End Crimped</h3>
          <p class="text-text-secondary mb-6">
            Both ends crimped with terminals. Ready for immediate connector-to-connector assembly.
          </p>
          <ul class="space-y-2 text-text-secondary text-sm">
            <li class="flex items-start gap-2">
              <span class="text-accent-green">‚ñ∏</span>
              <span>Terminals on both ends</span>
            </li>
            <li class="flex items-start gap-2">
              <span class="text-accent-green">‚ñ∏</span>
              <span>Male-to-female or same-type</span>
            </li>
            <li class="flex items-start gap-2">
              <span class="text-accent-green">‚ñ∏</span>
              <span>Quick disconnect capability</span>
            </li>
            <li class="flex items-start gap-2">
              <span class="text-accent-green">‚ñ∏</span>
              <span>Wire-to-wire assemblies</span>
            </li>
          </ul>
        </div>
      </div>
    </div>
  </section>

  <!-- Custom Specifications -->
  <section class="px-4 py-16 bg-bg-secondary border-y border-border">
    <div class="max-w-7xl mx-auto">
      <div class="mb-12">
        <p class="font-mono text-xs text-accent-cyan uppercase tracking-widest mb-3">Custom Manufacturing</p>
        <h2 class="text-3xl font-bold tracking-tight">Customization Options</h2>
        <p class="text-text-secondary mt-4 max-w-3xl">
          All assemblies are manufactured to your exact specifications. Provide your drawing or requirements
          for custom configurations.
        </p>
      </div>

      <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-6">
        {customSpecs.map(spec => (
          <div class="bg-bg-tertiary border border-border p-6">
            <div class="font-mono text-xs text-text-muted uppercase tracking-wider mb-2">{spec.label}</div>
            <div class="font-mono text-2xl font-bold text-accent-cyan mb-1">{spec.range}</div>
            <div class="text-text-secondary text-sm">{spec.unit}</div>
          </div>
        ))}
      </div>
    </div>
  </section>

  <!-- Quality Assurance -->
  <section class="px-4 py-16">
    <div class="max-w-7xl mx-auto">
      <div class="mb-12">
        <p class="font-mono text-xs text-accent-cyan uppercase tracking-widest mb-3">Quality Control</p>
        <h2 class="text-3xl font-bold tracking-tight">Testing & Inspection</h2>
        <p class="text-text-secondary mt-4 max-w-3xl">
          Every pre-crimped wire assembly undergoes rigorous testing to ensure consistent quality and reliable performance.
        </p>
      </div>

      <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-6">
        {qualityTests.map(test => (
          <div class="bg-bg-secondary border border-border p-6">
            <div class="text-3xl mb-3">{test.icon}</div>
            <h3 class="font-bold mb-2">{test.name}</h3>
            <p class="text-text-secondary text-sm">{test.desc}</p>
          </div>
        ))}
      </div>
    </div>
  </section>

  <!-- CTA Section -->
  <section class="px-4 py-20 bg-bg-secondary border-y border-accent-cyan">
    <div class="max-w-4xl mx-auto text-center">
      <h2 class="text-3xl md:text-4xl font-bold mb-6">Request Custom Pre-Crimped Wire Quote</h2>
      <p class="text-lg text-text-secondary mb-8 max-w-2xl mx-auto">
        Upload your assembly drawing or specifications. Our engineering team will review your requirements
        and provide pricing and lead time within 24 hours.
      </p>

      <div class="flex flex-wrap justify-center gap-4 mb-8">
        <div class="flex items-center gap-2">
          <span class="text-accent-green">‚úì</span>
          <span class="text-sm">Free specification review</span>
        </div>
        <div class="flex items-center gap-2">
          <span class="text-accent-green">‚úì</span>
          <span class="text-sm">24-hour quote response</span>
        </div>
        <div class="flex items-center gap-2">
          <span class="text-accent-green">‚úì</span>
          <span class="text-sm">Prototype samples available</span>
        </div>
        <div class="flex items-center gap-2">
          <span class="text-accent-green">‚úì</span>
          <span class="text-sm">Volume discounts</span>
        </div>
      </div>

      <a href="/contact" class="inline-block px-10 py-4 font-mono text-sm font-bold uppercase tracking-widest text-bg-primary bg-accent-cyan hover:bg-accent-green transition-all duration-300 hover:-translate-y-0.5 hover:shadow-lg hover:shadow-accent-green/30">
        Upload Drawing for Quote
      </a>

      <div class="mt-8 text-text-secondary text-sm">
        <p>Minimum order quantity: 100 pieces</p>
        <p class="mt-1">Lead time: 7-14 days depending on specifications</p>
      </div>
    </div>
  </section>

  <!-- Related Assembly Types -->
  <section class="px-4 py-16">
    <div class="max-w-7xl mx-auto">
      <div class="mb-12">
        <p class="font-mono text-xs text-accent-cyan uppercase tracking-widest mb-3">Other Assembly Types</p>
        <h2 class="text-3xl font-bold tracking-tight">Related Cable Assemblies</h2>
      </div>

      <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-6">
        <a href="/products/wire-to-board-cable-assembly" class="group block bg-bg-secondary border border-border p-6 transition-all duration-300 hover:border-accent-cyan">
          <h3 class="font-bold mb-2 group-hover:text-accent-cyan">Wire-to-Board Cable Assembly</h3>
          <p class="text-text-secondary text-sm">Cable assemblies with connectors for PCB mounting applications.</p>
          <div class="mt-4 flex items-center text-accent-cyan font-mono text-sm">
            <span>Learn More</span>
            <span class="ml-2 group-hover:translate-x-1 transition-transform">‚Üí</span>
          </div>
        </a>

        <a href="/products/wire-to-wire-cable-assembly" class="group block bg-bg-secondary border border-border p-6 transition-all duration-300 hover:border-accent-cyan">
          <h3 class="font-bold mb-2 group-hover:text-accent-cyan">Wire-to-Wire Cable Assembly</h3>
          <p class="text-text-secondary text-sm">Male-to-female cable assemblies for modular connections.</p>
          <div class="mt-4 flex items-center text-accent-cyan font-mono text-sm">
            <span>Learn More</span>
            <span class="ml-2 group-hover:translate-x-1 transition-transform">‚Üí</span>
          </div>
        </a>

        <a href="/oem-wire-cable-assembly-manufacturer" class="group block bg-bg-secondary border border-border p-6 transition-all duration-300 hover:border-accent-cyan">
          <h3 class="font-bold mb-2 group-hover:text-accent-cyan">OEM Manufacturing Services</h3>
          <p class="text-text-secondary text-sm">Full OEM manufacturing capabilities for B2B clients.</p>
          <div class="mt-4 flex items-center text-accent-cyan font-mono text-sm">
            <span>Learn More</span>
            <span class="ml-2 group-hover:translate-x-1 transition-transform">‚Üí</span>
          </div>
        </a>
      </div>
    </div>
  </section>
</MainLayout>
